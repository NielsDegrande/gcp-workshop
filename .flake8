[flake8]

application-import-names = gcp-workshop
exclude =
    __pycache__,
    *.egg-info,
    *.pyc,
    .cache,
    .eggs,
    .git,
    .mypy_cache,
    .pytest_cache,
    .tox,
    .venv,
    build,
    dist,
    tmp,
import-order-style = pep8
max-complexity = 10
max-line-length = 88
output-file = tmp/flake8.log
tee = True
per-file-ignores =
    __init__.py:D104, F401
